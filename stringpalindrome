

import java.util.*;

public class palindrom {
    public static boolean isPalindrome(String str) {
        int n = str.length();

        for (int i = 0; i < n; i++) {
            if (str.charAt(i) == str.charAt(n - i - 1)) {
                return true;
            }

        }
        return false;
    }

    public static void main(String args[]) {
        String str = "Sagnik";
        System.out.println(isPalindrome(str));
    }

}
